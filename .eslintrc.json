{
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true,
    "jasmine": true,
    "mocha": true,
    "qunit": true 
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "globalReturn": true
    },
    "sourceType": "script"
  },
  "globals": {
    "ActiveXObject": true,
    "XDomainRequest": true,
    "ScriptEngine": true,
    "WSH": true,
    "WScript": true,
    "DocumentTouch": true
  }
}

